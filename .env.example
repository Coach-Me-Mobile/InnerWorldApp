# InnerWorld Local Development Environment
# Copy this file to .env and optionally add your API key

# Environment
ENVIRONMENT=development
DEBUG=true

# API Key (Optional - app works with mocks if not provided)
OPENROUTER_API_KEY=your-openrouter-api-key-here

# LLM Configuration  
OPENROUTER_BASE_URL=https://openrouter.ai/api/v1
OPENROUTER_MODEL=openai/gpt-4o-mini

# Local Services
LOCALSTACK_ENDPOINT=http://localhost:4566
